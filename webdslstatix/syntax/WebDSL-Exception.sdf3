module WebDSL-Exception

imports
  WebDSL-Action
  WebDSL-Lexical
  WebDSL

context-free sorts

  Catch

context-free syntax

  Statement.ThrowStatement = "throw" {Exp ","}+ ";" {prefer}
  TemplateCall.ThrowTemplate = "throw" {Exp ","}+ {prefer}
  Statement.TryStatement = "try" Block Catch+ {prefer}
  TemplateCall.TryTemplate = "try" Block Catch+ {prefer}
  Catch.Catch = "catch" "(" Id ":" Sort ")" Block 
