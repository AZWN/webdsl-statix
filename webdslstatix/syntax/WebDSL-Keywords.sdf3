module WebDSL-Keywords

imports
  WebDSL-Lexical

context-free restrictions

  "as" -/- [a-zA-Z0-9\_]

lexical syntax // id

  Id = "throw" {reject}
  Id = "try" {reject}

  Id = "this" {reject}

  Id = "namespace" {reject}

lexical syntax // var id

  VarId = "true" {reject}
  VarId = "false" {reject}
  VarId = "null" {reject}
  // VarId = "global" {reject}
  // VarId = "session" {reject}
  // VarId = "default" {reject}
  VarId = "function" {reject}
  VarId = "return" {reject}
  VarId = "action" {reject}

  VarId = "List" {reject}
  VarId = "Set" {reject}

  VarId = "id" {reject}

lexical syntax // fun id

//  FunId = "goto" {reject}

